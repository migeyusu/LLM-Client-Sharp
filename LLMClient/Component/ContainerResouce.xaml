<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:data="clr-namespace:LLMClient.Data"
                    xmlns:abstraction="clr-namespace:LLMClient.Abstraction"
                    xmlns:extension="clr-namespace:LLMClient.Component.Extension">
    <extension:Service x:Key="PromptsResource" ServiceType="{x:Type data:IPromptsResource}" />
    <extension:Service x:Key="RagSourceCollection" ServiceType="{x:Type abstraction:IRagSourceCollection}" />
    <extension:Service x:Key="EndpointService" ServiceType="{x:Type abstraction:IEndpointService}" />
</ResourceDictionary>